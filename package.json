{
  "name": "sanity-nextjs-dashboard",
  "private": "true",
  "version": "1.0.0",
  "description": "Nextjs + Sanity.io : A Powerful Website Starter with Real-time Visual Editing",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "import-sample-data": "cd studio && sanity dataset import sample-data.tar.gz --replace"
  },
  "keywords": [
    "Sanity",
    "Next.js",
    "Headless CMS",
    "Visual Editing",
    "Live Content API (LCAPI)",
    "Real-time"
  ],
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-template-nextjs-clean/issues"
  },
  "homepage": "https://github.com/sanity-io/sanity-template-nextjs-clean#readme",
  "workspaces": [
    "studio",
    "frontend"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "react-is": "^19.1.0"
  }
}
